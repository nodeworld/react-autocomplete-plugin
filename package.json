{
  "name": "react-autocomplete-plugin",
  "version": "2.0.0",
  "author": "nodeworld",
  "description": "A simple, powerful, lightweight and customizable autocomplete tool programmed for React projects!",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "sh ./scripts/start.sh",
    "tsc":"./node_modules/typescript/bin/tsc",
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "test": "jest --watchAll --verbose"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/tests/styleMock.ts"
    }
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "postcss": {
    "plugins": {
      "postcss-import": {}
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^9.0.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.41",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.2.3",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.7.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nodeworld/react-autocomplete-plugin"
  },
  "homepage": "https://github.com/nodeworld/react-autocomplete-plugin#readme",
  "sponsor": {
    "url": "https://buymeacoffee.com/nodeworld"
  },
  "keywords": [
    "Autocomplete",
    "Auto complete",
    "React autocomplete",
    "Autosearch",
    "Auto search",
    "Autosuggest",
    "Auto suggest",
    "React autosuggest",
    "React autosearch",
    "React Autocomplete dropdown",
    "typeahead"
  ]
}
